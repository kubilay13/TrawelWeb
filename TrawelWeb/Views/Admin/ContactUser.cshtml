        <div class="card">
            <!--begin::Card header-->
            <div class="card-header border-0 pt-6">
                <!--begin::Card title-->
                <!--begin::Card title-->
            </div>
            <!--end::Card header-->
            <!--begin::Card body-->
            <div class="card-body pt-0 mw-650px">
                <!--begin::Table-->
                <table class="table align-middle table-row-dashed fs-6 gy-5" id="kt_table_ContactUserList">
                    <!--begin::Table head-->
                    <thead>
                        <!--begin::Table row-->
                        <tr class="text-start text-gray-400 fw-bolder fs-7 text-uppercase gs-0">
                            <th class="min-w-125px">S/N</th>
                            <th class="min-w-125px">Adı</th>
                            <th class="min-w-125px">Soyadı</th>
                            <th class="min-w-125px">E-Mail</th>
                            <th class="min-w-125px">Konu</th>
                            <th class="min-w-125px">Mesaj</th>
                            <th class="min-w-125px">Durum</th>
                            <th class="min-w-125px">İşlemler</th>
                        </tr>
                        <!--end::Table row-->
                    </thead>
                    <!--end::Table head-->
                    <!--begin::Table body-->
                    <!--end::Table body-->
                </table>
                <!--end::Table-->
            </div>
