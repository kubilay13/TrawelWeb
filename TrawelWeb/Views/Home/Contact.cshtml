@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<section class="w3l-about-breadcrumb text-left">
    <div class="breadcrumb-bg breadcrumb-bg-about py-sm-5 py-4">
        <div class="container py-2">
            <h2 class="title">Contact Us</h2>
            <ul class="breadcrumbs-custom-path mt-2">
                <li><a href="#url">Home</a></li>
                <li class="active"><span class="fa fa-arrow-right mx-2" aria-hidden="true"></span> Contact </li>
            </ul>
        </div>
    </div>
</section>

<section class="w3l-contact" id="contact">
    <div class="contact-infubd py-5">
        <div class="container py-lg-3">
            <div class="contact-grids row">
                <div class="col-lg-6 contact-left">
                    <div class="partners">
                        <div class="cont-details">
                            <h5>Get in touch</h5>
                            <p class="mt-3 mb-4">
                                Hi there, We are available 24/7 by fax, e-mail or by phone. Drop us line so we can talk
                                futher about that.
                            </p>
                        </div>
                        <div class="hours">
                            <h6 class="mt-4">Email:</h6>
                            <p>
                                <a href="mailto:mail@traversal.com">
                                    strattonoakmount89@gmail.com
                                </a>
                            </p>
                            <h6 class="mt-4">Visit Us:</h6>
                            <p> 78-80 Upper St Giles St. Norwich NR2 1LT United Kingdom.</p>
                            <h6 class="mt-4">Contact:</h6>
                            <p class="margin-top"><a href="tel:+44-255-366-88">+44-255-366-88</a></p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 mt-lg-0 mt-5 contact-right">
                    <form action="https://sendmail.w3layouts.com/submitForm" method="post" class="signin-form">
                        <div class="input-grids">
                            <div class="form-group">
                                <input type="text" name="FirstName" id="FirstName" placeholder="İsim" class="contact-input">
                            </div>
                            <div class="form-group">
                                <input type="text" name="LastName" id="LastName" placeholder="Soyisim" class="contact-input">
                            </div>
                            <div class="form-group">
                                <input type="email" name="Email" id="Email" placeholder="Email" class="contact-input" required="">
                            </div>
                            <div class="form-group">
                                <input type="text" name="Subject" id="Subject" placeholder="Başlık" class="contact-input">
                            </div>
                        </div>
                        <div class="form-group">
                            <textarea name="Message" id="Message" placeholder="Mesajınız" required=""></textarea>
                        </div>
                        <div class="text-right">
                            <a class="btn btn-style btn-secondary text-white" onclick="SendMessage()">Gönder</a>
                        </div>
                    </form>
                </div>

            </div>
            <div class="map mt-5 pt-md-5">
                <h5>Map</h5>
                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d12042.642853237112!2d28.959415717382818!3d41.010798800000025!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14cab9d460db69fb%3A0x8093fe1e1d4d1c4e!2zQ29pbnRyYWwgLSBLYXBhbMSxIMOHYXLFn8SxIEJpdGNvaW4!5e0!3m2!1str!2str!4v1701850636087!5m2!1str!2str" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
            </div>
        </div>
    </div>
</section>
<script src="~/js/index.js"></script>
<script src="~/js/theme-change.js"></script>